{
  "extends": "@hyuan/typescript-config",
  "compilerOptions": {
    "composite": true,
    "paths": {
      "@config": [
        "./config.ts"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@packages/*": [
        "../packages/*"
      ],
    }
  },
  "include": [
    "types/**/*.d.ts",
    "src/.vuepress/**/*.ts",
    "src/**/*.ts",
    "src/**/*.vue",
    "./config.ts"
  ],
  "exclude": [
    "src/.vuepress/.temp",
    "src/.vuepress/.cache"
  ]
}