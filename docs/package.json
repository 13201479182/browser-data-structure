{
  "name": "@hyuan/docs",
  "version": "0.0.1",
  "description": "A VuePress project",
  "license": "MIT",
  "type": "module",
  "main": "./config.ts",
  "scripts": {
    "docs:dev": "vuepress dev src -c=./src/.vuepress/server.ts --clean-cache",
    "docs:build": "vuepress build src -c=./src/.vuepress/server.ts --clean-cache",
    "docs:preview": "vite preview --outDir=dist",
    "lint": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "vite": "catalog:",
    "vue-tsc": "catalog:",
    "typescript": "catalog:",
    "vue": "catalog:",
    "vuepress": "catalog:vue-press",
    "vuepress-theme-hope": "catalog:vue-press",
    "vuepress-plugin-search-pro": "catalog:vue-press",
    "sass-embedded": "catalog:vue-press",
    "@vuepress/bundler-vite": "catalog:vue-press"
  }
}
